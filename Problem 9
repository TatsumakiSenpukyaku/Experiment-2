#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int row, columns;
	
	cout << "How many rows: ";
	cin >> row;
	cout << "How many columns: ";
	cin >> columns;
	
	cout << "\n";
	for (int i=0;i<row;i++)
	{
		for (int j=0;j<columns;j++)
		{
		cout << "*"; 
		}
		
		cout << "\n";
	}
	_getch();
	return 0;
}
