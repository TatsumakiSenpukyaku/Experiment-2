#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
	int num;
	
	cout << "Enter a number: ";
	cin >> num;
	cout << "Multiplication table of " << num << ": ";
	cout << "\n";
	for (int i=1;i<=10;i++)
	{
		cout << i << " * " << num << " = " << (num*i);
		cout << "\n";
	}
	_getch();
	return 0;	
}
