#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
	int number,anothernumber;
	
	for(number=1; number<=10; number++)
	{
		cout << number << ", ";
		
	}
	for(anothernumber=12; anothernumber<=30; anothernumber=anothernumber+2)
	{
		cout << anothernumber << ", ";
	}
	_getch();
	return 0;
}
